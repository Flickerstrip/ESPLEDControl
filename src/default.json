{
  "name": "FlickerstripApp",
  "version":"0.5.2",
  "description": "An application to allow you to interact with your LED strips.",
  "main": "nwjs.html",
  "showDebugger":false,
  "closeOnEsc":false,
  "patternRepository":"http://flickerstrip.elasticbeanstalk.com",
  "includedpatterns":"base",
  "window": {
    "title": "Flickerstrip",
    "icon": "icon.png",
    "toolbar":false,
    "frame": true,
    "min_width": 200,
    "min_height": 200,
    "width": 900,
    "height": 500,
    "resizable": true,
    "show_in_taskbar": true,
    "fullscreen": false
  },
  "dependencies": {
    "async": "0.9.0",
    "c-struct": "0.0.3",
    "jquery": "2.1.3",
    "request": "2.53.0",
    "requirejs": "2.1.17",
    "tinycolor2": "1.1.2",
    "underscore": "1.8.3",
    "wireless": "0.3.2",
    "extend": "3.0.0",
    "eventemitter2": "0.4.14",
    "node-ssdp": "2.6.2",
    "sandbox":"0.8.6",
	"jshint":"2.8.0",
    "get-pixels":"3.2.3",
    "yauzl":"2.4.1",
    "mkdirp":"0.5.1",
    "request-progress":"0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Flickerstrip/FlickerstripFirmware.git"
  }
}
