{
  "name": "wifiledcontrol",
  "version": "0.0.1",
  "description": "An application to allow you to interact with your LED strips.",
  "main": "./view/nwjs.html",
  "window": {
    "title": "WiFi LED Control",
    "icon": "icon.png",
    "toolbar": true,
    "frame": true,
    "min_width": 200,
    "min_height": 200,
    "width": 900,
    "height": 500,
    "resizable": true,
    "show_in_taskbar": true,
    "fullscreen": false
  },
  "dependencies": {
    "async": "0.9.0",
    "c-struct": "0.0.3",
    "jquery": "2.1.3",
    "python-shell": "0.0.3",
    "request": "2.53.0",
    "requirejs": "2.1.17",
    "serialport": "1.6.3",
    "tinycolor2": "1.1.2",
    "underscore": "1.8.3",
    "wireless": "0.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:julianh2o/ESPLEDControl.git"
  }
}
